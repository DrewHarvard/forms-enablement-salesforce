<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>URL, username and password for the Forms API Service.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Active__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Check this box to have the code use this instance of the custom setting. Check on only one instance. Allows values for DEV, PROD, etc to exist in one org.</inlineHelpText>
        <label>Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Password__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Used in the HTTP basic auth header sent in all requests to the Forms API Service.</inlineHelpText>
        <label>Password</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>StopService__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Prevent to continue pushing Forms to API. PLEASE BE AWERE IF TRUE NO FORMS WILL BE SEND TO CHIPS</inlineHelpText>
        <label>Stop Service</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>URL__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Full URL of the server, including scheme and path.</inlineHelpText>
        <label>URL</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>Username__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Used in the HTTP basic auth header sent in all requests to the Forms API Service.</inlineHelpText>
        <label>Username</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Forms API Service</label>
    <visibility>Public</visibility>
</CustomObject>
