    <apex:composition template="TransactionPageTemplate">
        <apex:define name="content">
            <apex:form >
                <apex:outputPanel >
                    
                        <div class="govuk-box-highlight">
                            <h1 class="bold-large">Sign out</h1>
                            <p>
                                You have now signed out
                                <br/>
                            </p>
                        </div>
                        
                    
                </apex:outputPanel>
            </apex:form>
        </apex:define>
    </apex:composition>
</apex:page>